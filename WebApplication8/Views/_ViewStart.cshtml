@{

    string currentUser = ViewContext.RouteData.Values["controller"].ToString();  //Convert.ToString(HttpContext.Request)
    Console.WriteLine("Here : ", currentUser);
    //
    @if (currentUser == "Home")
    {
        Layout = "_Layout";
    }
    else if (currentUser == "Admin")
    {
        Layout = "_AdminLayout";
    }
    else
    {
        Layout = "_CustomerLayout";
    }
    
}









